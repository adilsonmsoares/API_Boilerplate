{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=.;Initial Catalog=YOUR_DB;Integrated Security=True;MultipleActiveResultSets=True;"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "ElasticSearch": {
    "Uri": "http://localhost:9200",
    "Index": "logs-api-<nameapi>"
  },
  "Swagger": {
    "UiOn": true
  },
  "AllowedHosts": "*"
}